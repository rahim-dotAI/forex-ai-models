def print_status(msg, level="info"):
    icons = {"info":"‚ÑπÔ∏è","success":"‚úÖ","warn":"‚ö†Ô∏è","debug":"üêû","error":"‚ùå"}
    getattr(logging, level if level != "warn" else "warning", logging.info)(msg)
